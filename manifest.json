{
    "manifest_version": 3,
    "name":"CareerTrack",
    "description":"chrome extension for CareerTrack Web Application",
    "version":"1.0",
    "content_scripts":[
        {
            "css":["styles.css"],
            "matches":["https://localhost:7021/*"]
        }
    ],
    "host_permissions":[
        "https://localhost:7021/*",
        "http://localhost:5131/*"
    ],
    "permissions":[
        "activeTab"
    ],
    "action":{
        "default_popup":"action.html"
    }
}